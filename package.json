    "run-sequence": "^2.2.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-join": "^4.0.0",

{
  "name": "website",
  "version": "2.0.0",
  "repository": "https://github.com/hbsnow/website.git",
  "author": "hbsnow",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:html": "node build/metalsmith.js",
    "build:js": "webpack --config build/webpack.prod.conf.js ",
    "build:css": "postcss src/assets/css/main.css -o docs/assets/css/main.css -e production",
    "build:assets": "gulp build:assets",
    "prewatch": "npm run clean",
    "watch": "run-p build:assets watch:*",
    "watch:html": "cross-env NODE_ENV=development node build/metalsmith.js --watch",
    "watch:js": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "watch:css": "postcss src/assets/css/main.css -o docs/assets/css/main.css -e development -w -m",
    "watch:lint:css": "gulp watch:lint:css",
    "debug:html": "cross-env NODE_ENV=development DEBUG=metalsmith:* node build/metalsmith.js",
    "lint:js": "eslint --ext .js src",
    "lint:css": "gulp lint:css",
    "test": "jest",
    "clean": "rimraf ./docs"
  },
  "dependencies": {
    "autoprefixer": "^7.2.1",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "doiuse": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-imagemin": "^4.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-remember": "^0.3.1",
    "gulp-webp": "^2.3.0",
    "highlightjs": "^9.10.0",
    "htmlnano": "^0.1.6",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.1",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "jstransformer-pug": "^0.2.3",
    "metalsmith": "^2.3.0",
    "metalsmith-branch": "^0.0.5",
    "metalsmith-collection-metadata": "^0.1.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-copy": "^0.3.0",
    "metalsmith-debug": "^1.1.0",
    "metalsmith-drafts": "^0.0.1",
    "metalsmith-filemetadata": "^1.0.0",
    "metalsmith-if": "^0.1.1",
    "metalsmith-in-place": "^3.0.1",
    "metalsmith-json-metadata": "^0.1.0",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-markdownit": "^0.4.0",
    "metalsmith-posix-path": "^1.1.0",
    "metalsmith-posthtml": "^1.0.0",
    "metalsmith-rename": "^1.0.0",
    "metalsmith-rssfeed": "^0.0.6",
    "metalsmith-watch": "^1.0.3",
    "moment": "^2.19.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "page": "^1.7.1",
    "postcss-cli": "^4.1.1",
    "postcss-import": "^11.0.0",
    "posthtml-inline-assets": "^2.0.0",
    "recursive-escape": "^2.0.1",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.0",
    "stylelint": "^8.3.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-join": "^2.0.2",
    "vue": "^2.5.9",
    "vue-jest": "^1.3.0",
    "vue-loader": "^13.5.0",
    "vue-server-renderer": "^2.5.9",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}

{
  "name": "website",
  "version": "2.0.0",
  "repository": "https://github.com/hbsnow/website.git",
  "author": "hbsnow",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:html": "node build/metalsmith.js",
    "build:js": "webpack --config build/webpack.prod.conf.js",
    "build:gulp": "NODE_ENV=production gulp build",
    "prewatch": "npm run clean",
    "watch": "run-p watch:*",
    "watch:html": "NODE_ENV=development node build/metalsmith.js --watch",
    "watch:js": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "watch:format:js": "onchange '**/*.js' '!docs/**/*' -- prettier --write '**/*.js' '!docs/**/*'",
    "watch:gulp": "NODE_ENV=development gulp watch",
    "debug:html": "NODE_ENV=development DEBUG=metalsmith:* node build/metalsmith.js",
    "lint:js": "eslint --ext .js src",
    "format:js": "prettier --write '**/*.js' '!docs/**/*'",
    "lint:css": "gulp lint:css",
    "clean": "rimraf ./docs"
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "compare-url-hierarchy": "^1.0.0",
    "css-loader": "^0.28.11",
    "doiuse": "^4.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.2.0",
    "gulp-eslint": "^4.0.2",
    "gulp-filter": "^5.1.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-postcss": "^7.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-webp": "^3.0.0",
    "highlight.js": "^9.12.0",
    "htmlnano": "^0.1.7",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.1.0",
    "json-loader": "^0.5.7",
    "jstransformer-pug": "^0.3.0",
    "metalsmith": "^2.3.0",
    "metalsmith-branch": "^0.0.5",
    "metalsmith-collection-metadata": "^0.1.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-copy": "^0.4.0",
    "metalsmith-debug": "^1.2.0",
    "metalsmith-drafts": "^0.0.1",
    "metalsmith-filemetadata": "^1.0.0",
    "metalsmith-git": "^0.0.2",
    "metalsmith-if": "^0.1.1",
    "metalsmith-in-place": "^4.1.1",
    "metalsmith-json-metadata": "^0.1.0",
    "metalsmith-layouts": "^2.1.0",
    "metalsmith-markdownit": "mayo/metalsmith-markdownit#master",
    "metalsmith-posix-path": "^1.1.0",
    "metalsmith-posthtml": "^1.0.0",
    "metalsmith-rename": "^1.0.0",
